{
    "files.exclude": {
        "target/": true,
    },
    "cSpell.words": [
        "Azeri",
        "Belarussian",
        "Bokmal",
        "CONFIGFS",
        "eventfd",
        "functionfs",
        "Gbit",
        "iocb",
        "Johab",
        "Konkani",
        "Manipuri",
        "Mbit",
        "MSRV",
        "Rica",
        "RNDIS",
        "Sutu",
        "Tatarstan",
        "udcs",
        "wakeup"
    ],
    "cSpell.languageSettings": [
        {
            "languageId": "rust",
            "includeRegExpList": [
                "CStyleComment"
            ]
        }
